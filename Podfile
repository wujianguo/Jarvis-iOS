  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Jarvis
#  pod 'LeanCloud'
  pod 'LeanCloud', :path => '../swift-sdk'
  pod 'PKHUD'
  pod 'SnapKit'
  pod 'SQLite.swift'

  target 'JarvisTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'JarvisUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
